<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="mainBorderPane" prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.dma.controleur.MainControleur">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="Patients">
            <items>
              <MenuItem fx:id="menuItemAjoutPatient" mnemonicParsing="false" text="Ajout" />
              <MenuItem fx:id="menuItemSuppressionPatient" mnemonicParsing="false" text="Suppression" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Notes">
            <items>
              <MenuItem fx:id="menuItemNote" mnemonicParsing="false" text="Notes" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
